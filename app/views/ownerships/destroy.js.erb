<% if params[:type] == "Have" %>
  $(".have-<%= @item.id %>").replaceWith("<%= escape_javascript(cell(:items).(:have , item: @item)) %>");
<% else %>
  $(".want-<%= @item.id %>").replaceWith("<%= escape_javascript(cell(:items).(:want , item: @item)) %>");
<% end %>

$('.btn-have,.btn-want').removeAttr('disabled');